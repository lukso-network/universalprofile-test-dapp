{
  "name": "universalprofile-test-dapp",
  "version": "0.1.54",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --mode development",
    "build": "vue-tsc --noEmit && vite build",
    "build:cloudflare": "yarn lint:js && yarn lint:css && yarn lint:format && yarn lint:types && yarn test:unit && yarn build",
    "preview": "vite preview",
    "test:unit": "jest",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel 'lint:*!(fix)'",
    "lint:js": "eslint src --ext .js,.ts,.vue",
    "lint:css": "stylelint \"**/*.{css,scss,vue}\"",
    "lint:format": "prettier --check --loglevel=warn .",
    "lint:format:fix": "prettier --write --loglevel=warn .",
    "lint:types": "tsc --noEmit",
    "prepare": "husky install"
  },
  "dependencies": {
    "@erc725/erc725.js": "^0.17.2",
    "@lukso/lsp-factory.js": "2.5.1",
    "@lukso/lsp-smart-contracts": "^0.8.2",
    "@walletconnect/encoding": "^1.0.2",
    "@walletconnect/ethereum-provider": "^2.4.4",
    "@walletconnect/qrcode-modal": "1.8.0",
    "@walletconnect/sign-client": "2.2.1",
    "@walletconnect/types": "2.2.1",
    "@walletconnect/utils": "2.2.1",
    "@walletconnect/web3-provider": "1.8.0",
    "@web3modal/ethereum": "^2.1.1",
    "@web3modal/standalone": "^2.1.1",
    "bulma": "0.9.4",
    "eth-rpc-errors": "4.0.3",
    "ethers": "^5.7.2",
    "filesize": "10.0.7",
    "https-browserify": "1.0.0",
    "siwe": "1.1.6",
    "vue": "^3.2.47",
    "vue-router": "4.1.6",
    "wagmi": "^1.0.0",
    "web3": "1.9.0",
    "web3-eth-contract": "^1.9.0"
  },
  "devDependencies": {
    "@depay/web3-mock": "^13.26.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/user-event": "14.4.3",
    "@testing-library/vue": "6.6.1",
    "@types/jest": "28.1.8",
    "@typescript-eslint/eslint-plugin": "5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vue/compiler-sfc": "3.2.47",
    "@vue/vue3-jest": "28.1.0",
    "eslint": "8.38.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-vue": "9.10.0",
    "husky": "^8.0.3",
    "jest": "28.1.3",
    "jest-environment-jsdom": "28.1.3",
    "lint-staged": "^13.2.2",
    "lokijs": "^1.5.12",
    "npm-run-all": "4.1.5",
    "postcss-html": "^1.5.0",
    "prettier": "2.8.8",
    "sass": "1.61.0",
    "stream-browserify": "3.0.0",
    "stream-http": "3.2.0",
    "stylelint": "15.7.0",
    "stylelint-config-prettier": "9.0.5",
    "stylelint-config-recommended-vue": "1.4.0",
    "stylelint-config-standard-scss": "9.0.0",
    "ts-jest": "28.0.8",
    "typescript": "^5.0.4",
    "util": "0.12.5",
    "vite": "^4.2.1",
    "vue-loader": "17.0.1",
    "vue-tsc": "^1.3.12"
  },
  "lint-staged": {
    "*.{ts,js,vue}": [
      "prettier --list-different",
      "eslint"
    ],
    "*.json": "prettier --list-different",
    "*.ya?ml": "prettier --list-different",
    "*.{scss,css,vue}": [
      "prettier --list-different",
      "stylelint"
    ]
  },
  "engines": {
    "node": "18.12.1"
  },
  "packageManager": "yarn@3.3.0",
  "resolutions": {
    "wagmi": "~1.2.0"
  }
